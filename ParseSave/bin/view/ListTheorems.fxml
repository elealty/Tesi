<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>
 
<?import fxmltableview.*?> 


<GridPane alignment="CENTER" hgap="10.0" vgap="10.0"
    xmlns:fx="http://javafx.com/fxml"
    fx:controller="controller.TheoremListController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label style="-fx-font: NORMAL 20 Tahoma;" text="Theorem's List"                
        GridPane.columnIndex="0" GridPane.rowIndex="0">
    </Label>
    <TableView fx:id="tableViewTheorems" GridPane.columnIndex="0" 
        GridPane.rowIndex="1">
        <columns>
         <TableColumn prefWidth="75.0" text="ID" fx:id="colId" />
         <TableColumn text="Name"/>
         <TableColumn text="Provable"/>
         <TableColumn text="Time"/>
     </columns>   
    </TableView>
</GridPane>