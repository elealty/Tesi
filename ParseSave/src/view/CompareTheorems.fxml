<GridPane alignment="CENTER" focusTraversable="true" hgap="10.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="1000.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CompareController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
   <children>
       <Label prefHeight="13.0" prefWidth="226.0" style="-fx-font: NORMAL 20 Tahoma;" text="Compare theorems" GridPane.columnIndex="0" GridPane.rowIndex="0">
    </Label>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="1009.0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS">
         <children>
            <VBox prefHeight="514.0" prefWidth="282.0">
               <children>
                  <Label prefHeight="28.0" prefWidth="279.0" text="Filter theorems" />
                  <VBox prefHeight="56.0" prefWidth="282.0">
                     <children>
                        <Label text="Machine" />
                        <ComboBox fx:id="cmbMachine" prefHeight="25.0" prefWidth="246.0" />
                     </children>
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </VBox>
                  <VBox prefHeight="300.0" prefWidth="282.0">
                     <children>
                        <Label text="Testsets" />
                        <ListView fx:id="listTestset" prefHeight="283.0" prefWidth="271.0">
                           <padding>
                              <Insets right="5.0" />
                           </padding></ListView>
                     </children>
                  </VBox>
                  <Label text="Use Ctrl + mouse click to multi select">
                     <font>
                        <Font size="11.0" />
                     </font>
                  </Label>
            <Button fx:id="buttonSearch" alignment="CENTER" disable="true" mnemonicParsing="false" onAction="#handleSearchAction" text="Search" wrapText="true">
                     <VBox.margin>
                        <Insets left="10.0" top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="537.0" prefWidth="700.0" HBox.hgrow="ALWAYS">
               <tabs>
                  <Tab text="Theorems">
                     <content> 
                  <TableView fx:id="tableViewComparedTheorem" editable="true" maxHeight="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="500.0">
                        <columns>
                        <TableColumn fx:id="itemProverCompareCol" prefWidth="127.0" text="Prover" />
                        <TableColumn fx:id="itemNameCompareCol" prefWidth="127.0" text="Name" />
                        <TableColumn fx:id="itemProvableCompareCol" maxWidth="80.0" minWidth="50.0" text="Provable" />
                        <TableColumn fx:id="itemExecutionCompareCol" prefWidth="135.0" text="Execution (ms)" />
                   </columns>   
                  </TableView>
                     </content>
                  </Tab>
                  <Tab text="Charts">
                     <content>
                        <VBox>
                           <children>
                              <ScrollPane fx:id="scrollPaneChart" prefViewportHeight="513.0" prefViewportWidth="690.0">
                                 <content>
                                 <BarChart fx:id="executionChart" barGap="2.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="481.0" prefWidth="674.0" title="Execution" HBox.hgrow="ALWAYS">
                                    <xAxis>
                                      <CategoryAxis focusTraversable="true" label="theorems" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" />
                                    </xAxis>
                                    <yAxis>
                                      <NumberAxis label="time" side="LEFT" />
                                    </yAxis>
                                  </BarChart>
                                 </content>
                              </ScrollPane>
                              <Label text="Use Ctrl + mouse to zoom" />
                           </children>
                        </VBox>
                     </content></Tab>
                  
               </tabs>
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </TabPane>
         </children>
      </HBox>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
