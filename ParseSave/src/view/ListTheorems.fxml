   <children>
       <Label prefHeight="30.0" prefWidth="152.0" style="-fx-font: NORMAL 20 Tahoma;" text="Theorem's List">
    </Label>
      <HBox prefHeight="40.0" prefWidth="452.0" GridPane.rowIndex="1">
         <children>
            <Label contentDisplay="BOTTOM" text="Search:" />
