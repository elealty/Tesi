<GridPane alignment="CENTER" hgap="10.0" prefHeight="441.0" prefWidth="652.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TheoremListController">
